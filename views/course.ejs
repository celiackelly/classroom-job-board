<section class="container-fluid p-5">
    <h1>This is a course page for <%= course.name %></h1>

    <% if (students.length) { %>
        <% students.forEach(student => { %>
            <p><%= student.firstName %> <%= student.lastName %></p>
            <% })
        %>
        <%} else { %> 
            <p> Add some students to get started.</p>
        <%}
    %> 
</section>
